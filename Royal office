        const elements = document.querySelectorAll('.elementor-widget-heading, .elementor-widget-text-editor, .elementor-widget-breadcrumbs, .wp-block-paragraph, .post-content-search .entry-title a');
        
        elements.forEach(element => {
            let content = element.innerHTML;

            wordsToProtect.forEach(word => {
                const regex = new RegExp('(' + word + ')(?![^<]*(<\/a>|alt="[^"]*"))', 'g');
                content = content.replace(regex, '<span class="no-break">$1</span>');
            });
            element.innerHTML = content;
        });
    });
    </script>
